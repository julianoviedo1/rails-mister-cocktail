<div class="container card m-2 p-2">
  <%= simple_form_for [@cocktail] do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%#= f.input :password, input_html: { autocomplete: 'current-password' } %>
    <%= f.submit "Submit", class: "btn btn-flat" %>
    <%= link_to 'Return', cocktails_path, class: "btn btn-back" %>
  <% end %>
</div>